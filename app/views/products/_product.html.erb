<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Local:</strong>
    <%= product.local %>
  </p>

  <p>
    <strong>Sell price:</strong>
    <%= product.sell_price %>
  </p>

  <p>
    <strong>Buy price:</strong>
    <%= product.buy_price %>
  </p>

  <p>
    <strong>Unit:</strong>
    <%= product.unit %>
  </p>

</div>
