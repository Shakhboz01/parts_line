<div class="card shadow">
  <div class="card-header d-flex justify-content-between align-items-center">
    <h3 class="card-title font-weight-bold">Приход товаров <%= clink_to 'Изменить вид', delivery_from_counterparties_path, 'fa-eye' %></h3>
    <%= link_to 'Добавить новый', new_product_entry_path, class: "btn btn-success min-width-btn" %>
  </div>
  <%= render 'filter_form', q: @q, url: request.path %>
  <div class="card-body">
    <%= render 'product_entries/product_entries', q: @q, product_entries: @product_entries %>
  </div>
  <%= paginate @product_entries %>
</div>
